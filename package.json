{
  "name": "tree-sitter-runt",
  "version": "0.1.0",
  "description": "Tree-sitter grammar for Runt",
  "keywords": [
    "tree-sitter",
    "parser",
    "runt"
  ],
  "files": [
    "grammar.js",
    "queries/*",
    "src/**"
  ],
  "author": "Jarred",
  "license": "MIT",
  "devDependencies": {
    "tree-sitter-cli": "^0.22.0"
  },
  "scripts": {
    "build": "tree-sitter generate && tree-sitter build",
    "test": "tree-sitter test",
    "parse": "tree-sitter parse"
  },
  "tree-sitter": [
    {
      "scope": "source.runt",
      "injection-regex": "^runt$",
      "file-types": [
        "runt"
      ],
      "highlights": "queries/highlights.scm"
    }
  ]
}
